@model IEnumerable<WebApplication.Models.AspVideoDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VideoTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChannelId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChannelTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PublishedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VideoTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChannelId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChannelTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PublishedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VideoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.VideoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VideoId })
        </td>
    </tr>
